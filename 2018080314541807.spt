ghw.ajax.get(ctxPath+"/app_config/get_appsharelink?appId="+id, function (resp) {
            if(resp.code == resp_status.success) {
                $("#defaultLink").html(resp.data)
                appConfig.appShareHtmlModel.modal('show');
            }else {
                ghw.util.msgAlert({message: "获取游戏详情失败",type:"danger"});
            }
        })